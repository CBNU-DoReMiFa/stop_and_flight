<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#8DA8E3FF"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="23dp">

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="109dp">

            <TextView
                android:id="@+id/textView11"
                android:layout_width="258dp"
                android:layout_height="81dp"
                android:layout_marginStart="150dp"
                android:layout_marginEnd="150dp"
                android:background="@drawable/rectangle2"
                android:gravity="center"
                android:text="나의 비행기록"
                android:textColor="#021B95"
                android:textSize="36sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="11dp">

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/day"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_gravity="center"
            android:layout_margin="5dp"
            android:layout_marginTop="680dp"
            android:background="@drawable/rectangle"
            android:backgroundTint="#769FCD"
            android:gravity="center"
            android:shadowColor="#000000"
            android:textColor="#FFFFFF"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/todo"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:layout_margin="5dp"
            android:layout_marginTop="680dp"
            android:background="@drawable/rectangle"
            android:backgroundTint="#FFEE80"
            android:gravity="center"
            android:hint="@string/no_todo"
            android:textColor="#871818"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/time"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:layout_margin="5dp"
            android:layout_marginTop="680dp"
            android:background="@drawable/rectangle"
            android:backgroundTint="#FFEE80"
            android:gravity="center"
            android:hint="@string/no_time"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/star"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_margin="5dp"
            android:layout_marginTop="680dp"
            android:background="@drawable/rectangle"
            android:backgroundTint="#FFEE80"
            android:gravity="center"
            android:hint="@string/no_star"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/memo"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_margin="5dp"
            android:layout_marginTop="680dp"
            android:background="@drawable/rectangle"
            android:backgroundTint="#FFEE80"
            android:gravity="center"
            android:hint="@string/no_note"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/bottom"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_gravity="center"
            android:layout_margin="5dp"
            android:layout_marginTop="680dp"
            android:background="@drawable/rectangle"
            android:backgroundTint="#769FCD"
            android:gravity="center"
            android:shadowColor="#000000"
            android:textColor="#FFFFFF"
            android:textSize="18sp" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="30dp">

        </androidx.constraintlayout.widget.ConstraintLayout>

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="match_parent"
            android:layout_height="136dp"
            app:srcCompat="@drawable/icons8_airplane_take_off_96" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>